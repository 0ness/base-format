<body id="@@(ページid)@@"@@((ページclass == '')?'' : ' class="' + ページclass + '"';)@@>
<!-- ============= wrapper ============= -->
<div id="wrapper"> 
	<!-- header -->
	<header id="header"> 
		<!-- gnav --> 
		<nav id="gNav">
			<ul>
				<li id="gn_01"@@((ページid != 'category_id_01') ? '' : ' class="current"';)@@><a href="#header"></a></li>
				<li id="gn_02"@@((ページid != 'category_id_02') ? '' : ' class="current"';)@@><a href="#header"></a></li>
				<li id="gn_03"@@((ページid != 'category_id_03') ? '' : ' class="current"';)@@><a href="#header"></a></li>
				<li id="gn_04"@@((ページid != 'category_id_04') ? '' : ' class="current"';)@@><a href="#header"></a></li>
				<li id="gn_05"@@((ページid != 'category_id_05') ? '' : ' class="current"';)@@><a href="#header"></a></li>
			</ul>
		</nav>
		<!-- /gnav --> 
	</header>
	<!-- /header --> 
	
	<!-- ============= contents ============= -->
	<div id="contents"> 
		<!-- main -->
		<main id="main"><!-- TemplateBeginEditable name="メイン部分" -->メイン部分<!-- TemplateEndEditable --></main>
		<!-- /main --> 
		<!-- sub -->
		<div id="sub"> 
			<!-- subNav -->
			<nav id="subNav"> <!-- TemplateBeginIf cond="_document['subNav-contents']" --> 
				<!-- subNav-about -->
				<dl>
					<dt>カテゴリータイトル</dt>
					<dd@@((ページid != 'content_id_01') ? '' : ' class="current"';)@@><a href="#header"></a>
					</dd>
					<dd@@((ページid != 'content_id_02') ? '' : ' class="current"';)@@><a href="#header"></a>
					</dd>
					<dd@@((ページid != 'content_id_03') ? '' : ' class="current"';)@@><a href="#header"></a>
					</dd>
					<dd@@((ページid != 'content_id_04') ? '' : ' class="current"';)@@><a href="#header"></a>
					</dd>
				</dl>
				<!-- /subNav-about --> 
				<!-- TemplateEndIf --> </nav>
		</div>
		<!-- /sub --> 
		<a href="#header" id="top_back">トップに戻る</a> </div>
	<!-- /============= contents ============= --> 
	
	<!-- footer -->
	<footer id="footer"> 
		<!-- <ul>
			<li><a href="#header">サイトマップ</a></li>
